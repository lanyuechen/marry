{
  "name": "页面元素",
  "description": "不同页面元素展示",
  "cover": "./images/animals/7.jpg",
  "tags": ["演示", "测试"],
  "transition": {
    "direction": "horizontal",
    "effect": "slide",
    "autoPlay": false
  },
  "pages": [
    {
      "background": "#ddd",
      "elements": [
        {
          "type": "image",
          "props": {
            "src": "./images/animals/1.jpg"
          },
          "position": [0, 0],
          "size": ["100%", 300],
          "animation": {
            "duration": 1000,
            "type": "slideInLeft"
          }
        },
        {
          "type": "text",
          "props": {
            "value": "文字文字文字文字文字文字"
          },
          "position": ["10%", 400],
          "size": ["80%", 100],
          "animation": {
            "duration": 1000,
            "delay": 500,
            "type": "slideInLeft"
          }
        },
        {
          "type": "icon",
          "props": {
            "type": "001-man"
          },
          "position": ["10%", 450],
          "size": [100, 100],
          "animation": {
            "duration": 1000,
            "delay": 1000,
            "type": "slideInLeft"
          }
        }
      ]
    }
  ]
}